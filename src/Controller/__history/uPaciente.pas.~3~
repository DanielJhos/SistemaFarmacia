unit uPaciente;

interface

type
  TPaciente = class
  private
    FID: Integer;
    FNome: String;
    FDatNascimento: TDate;
  public
    property ID: Integer read FID write FID;
    property Nome: String read FNome write FNome;
    property DataNascimento: TDate read FDataNascimento write FDataNascimento;
  end;

implementation

end.
